{
	    "source": {
	        "include": ["src", "DOCS.md"],
	        "includePattern": ".(js$|jsdoc$)",
	        "excludePattern": "(node_modules/|docs)"
	    },

	    "opts": {
	        "encoding": "utf8",
	        "readme": "./DOCS.md",
	        "destination": "docs/",
	        "recurse": true,
	        "verbose": true,
	        "template": "./node_modules/clean-jsdoc-theme",
	        "theme_opts": {
	            "default_theme": "dark"
	        }
	    }

}
